{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ],
    "headers": [
      {
        "source": "/:path*",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }